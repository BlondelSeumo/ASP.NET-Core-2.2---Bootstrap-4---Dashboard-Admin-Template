
@{
    ViewData["Title"] = "Offline";
}

<!-- [ Main Content ] start -->
<div class="pcoded-main-container">
    <div class="pcoded-wrapper">
        <div class="pcoded-content">
            <div class="pcoded-inner-content">
                <!-- [ breadcrumb ] start -->
                <div class="page-header">
                    <div class="page-block">
                        <div class="row align-items-center">
                            <div class="col-md-12">
                                <div class="page-header-title">
                                    <h5 class="m-b-10">Offline</h5>
                                </div>
                                <ul class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="/Dashboard/@Pages.Dashboard1.Value"><i class="feather icon-home"></i></a></li>
                                    <li class="breadcrumb-item"><a href="#!">Extra Components</a></li>
                                    <li class="breadcrumb-item"><a href="#!">Offline</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- [ breadcrumb ] end -->
                <div class="main-body">
                    <div class="page-wrapper">
                        <!-- [ Main Content ] start -->
                        <div class="row">
                            <!-- [ offline-section ] start -->
                            <div class="col-sm-12">
                                <div class="card">
                                    <div class="card-header">
                                        <h5>Offline Indicator</h5>
                                        <div class="card-header-right">
                                            <div class="btn-group card-option">
                                                <button type="button" class="btn dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                    <i class="feather icon-more-horizontal"></i>
                                                </button>
                                                <ul class="list-unstyled card-option dropdown-menu dropdown-menu-right">
                                                    <li class="dropdown-item full-card"><a href="#!"><span><i class="feather icon-maximize"></i> maximize</span><span style="display:none"><i class="feather icon-minimize"></i> Restore</span></a></li>
                                                    <li class="dropdown-item minimize-card"><a href="#!"><span><i class="feather icon-minus"></i> collapse</span><span style="display:none"><i class="feather icon-plus"></i> expand</span></a></li>
                                                    <li class="dropdown-item reload-card"><a href="#!"><i class="feather icon-refresh-cw"></i> reload</a></li>
                                                    <li class="dropdown-item close-card"><a href="#!"><i class="feather icon-trash"></i> remove</a></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="card-block">
                                        <div class="row">
                                            <div class="col-md-6 mb-2">
                                                <h5>Default</h5>
                                                <hr>
                                                <div class="browser offline-box">
                                                    <iframe data-theme="default"></iframe>
                                                </div>
                                            </div>
                                            <div class="col-md-6 mb-2">
                                                <h5>Slide</h5>
                                                <hr>
                                                <div class="browser offline-box">
                                                    <iframe data-theme="slide"></iframe>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="card-block">
                                        <div class="row">
                                            <div class="col-md-6 mb-2">
                                                <h5>Dark</h5>
                                                <hr>
                                                <div class="browser offline-box">
                                                    <iframe data-theme="dark"></iframe>
                                                </div>
                                            </div>
                                            <div class="col-md-6 mb-2">
                                                <h5>Chrome</h5>
                                                <hr>
                                                <div class="browser offline-box">
                                                    <iframe data-theme="chrome"></iframe>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="card">
                                    <div class="card-header">
                                        <h5>Indicator Theme</h5>
                                        <div class="card-header-right">
                                            <div class="btn-group card-option">
                                                <button type="button" class="btn dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                    <i class="feather icon-more-horizontal"></i>
                                                </button>
                                                <ul class="list-unstyled card-option dropdown-menu dropdown-menu-right">
                                                    <li class="dropdown-item full-card"><a href="#!"><span><i class="feather icon-maximize"></i> maximize</span><span style="display:none"><i class="feather icon-minimize"></i> Restore</span></a></li>
                                                    <li class="dropdown-item minimize-card"><a href="#!"><span><i class="feather icon-minus"></i> collapse</span><span style="display:none"><i class="feather icon-plus"></i> expand</span></a></li>
                                                    <li class="dropdown-item reload-card"><a href="#!"><i class="feather icon-refresh-cw"></i> reload</a></li>
                                                    <li class="dropdown-item close-card"><a href="#!"><i class="feather icon-trash"></i> remove</a></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="card-block">
                                        <div class="row">
                                            <div class="col-md-6 mb-2">
                                                <h5>Default</h5>
                                                <hr>
                                                <div class="browser offline-box">
                                                    <iframe data-theme="default-indicator"></iframe>
                                                </div>
                                            </div>
                                            <div class="col-md-6 mb-2">
                                                <h5>Slide</h5>
                                                <hr>
                                                <div class="browser offline-box">
                                                    <iframe data-theme="slide-indicator"></iframe>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="card-block">
                                        <div class="row">
                                            <div class="col-md-6 mb-2">
                                                <h5>Dark</h5>
                                                <hr>
                                                <div class="browser offline-box">
                                                    <iframe data-theme="dark-indicator"></iframe>
                                                </div>
                                            </div>
                                            <div class="col-md-6 mb-2">
                                                <h5>Chrome</h5>
                                                <hr>
                                                <div class="browser offline-box">
                                                    <iframe data-theme="chrome-indicator"></iframe>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- [ offline-section ] end -->
                        </div>
                        <!-- [ Main Content ] end -->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- [ Main Content ] end -->

@section Styles{
    <!-- offline css -->
    <link rel="stylesheet" href="~/assets/plugins/offline/css/offline-theme-slide.css">
    <link rel="stylesheet" href="~/assets/plugins/offline/css/offline-language-english.css">
}

@section Scripts{
    <!-- offline js -->
    <script src="~/assets/plugins/offline/js/offline.min.js"></script>
    <script src="~/assets/js/pages/offline-custom.js"></script>
}

