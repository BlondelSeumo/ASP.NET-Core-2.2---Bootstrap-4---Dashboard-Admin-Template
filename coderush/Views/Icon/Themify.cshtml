
@{
    ViewData["Title"] = "Themify";
}

<!-- [ Main Content ] start -->
<div class="pcoded-main-container">
    <div class="pcoded-wrapper">
        <div class="pcoded-content">
            <div class="pcoded-inner-content">
                <!-- [ breadcrumb ] start -->
                <div class="page-header">
                    <div class="page-block">
                        <div class="row align-items-center">
                            <div class="col-md-12">
                                <div class="page-header-title">
                                    <h5 class="m-b-10">Themify</h5>
                                </div>
                                <ul class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="/Dashboard/@Pages.Dashboard1.Value"><i class="feather icon-home"></i></a></li>
                                    <li class="breadcrumb-item"><a href="#!">Icons</a></li>
                                    <li class="breadcrumb-item"><a href="#!">Themify</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- [ breadcrumb ] end -->
                <div class="main-body">
                    <div class="page-wrapper">
                        <!-- [ Main Content ] start -->
                        <div class="row">
                            <!-- [ themify-icon ] start -->
                            <div class="col-sm-12">
                                <div class="card">
                                    <div class="card-header">
                                        <h5>Themify Icon</h5>
                                    </div>
                                    <div class="card-block text-center">
                                        <div class="row justify-content-center">
                                            <div class="col-sm-6">
                                                <input type="text" id="icon-search" class="form-control mb-4" placeholder="search . . ">
                                            </div>
                                        </div>
                                        <div class="i-main" id="icon-wrapper"></div>
                                    </div>
                                </div>
                            </div>
                            <!-- [ themify-icon ] end -->
                        </div>
                        <!-- [ Main Content ] end -->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- [ Main Content ] end -->

@section Styles{
    <!-- themify icon -->
    <link rel="stylesheet" href="~/assets/fonts/themify/themify.css">
}

